{
  "name": "highpoint",
  "version": "1.2.4",
  "main": "bin/server.js",
  "bin": "bin/server.js",
  "repository": {
    "url": "https://github.com/nozzlegear/highpoint",
    "type": "git"
  },
  "author": "Joshua Harms <joshua@nozzlegear.com>",
  "license": "MIT",
  "scripts": {
    "prebarrel": "dos2unix node_modules/barrelsby/bin/index.js",
    "barrel": "barrelsby -d ./routes --delete && barrelsby -d ./database --delete && barrelsby -d services --delete",
    "build": "webpack",
    "watch": "webpack --watch",
    "prebuild": "npm run barrel",
    "prewatch": "npm run prebuild",
    "prepackage": "cross-env-shell NODE_ENV=production npm run build && npm run uggo",
    "package": "pkg package.json --targets node8-linux --output highpoint.linux",
    "uggo": "npm run uggo:client && npm run uggo:mobile",
    "uggo:client": "uglifyjs -o dist/client.js --mangle --compress -- dist/client.js",
    "uggo:mobile": "uglifyjs -o dist/mobile.js --mangle --compress -- dist/mobile.js",
    "script:fill-db": "ts-node ./scripts/fill-db.ts",
    "script:migrate": "ts-node ./scripts/migrate.ts"
  },
  "pkg": {
    "scripts": [],
    "assets": [
      "bin/**/*.js",
      "dist/**/*.js",
      "dist/client.js",
      "dist/mobile.js",
      "images/**/*",
      "dist/css/**/*",
      "package.json",
      "index.html",
      "mobile.html"
    ]
  },
  "dependencies": {
    "auto-prop-component": "^1.9.2",
    "axios": "^0.15.3",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.4.6",
    "boom": "^4.2.0",
    "davenport": "^2.6.0",
    "express": "^4.14.0",
    "gearworks-cache": "^1.0.0",
    "gearworks-http": "^1.2.0",
    "gearworks-route": "^1.8.0-rc.3",
    "gearworks-validation": "^1.1.1",
    "import-to-array": "^1.0.0",
    "is-in-browser": "^1.0.2",
    "joi": "^9.2.0",
    "jwt-simple": "^0.5.1",
    "logspect": "^1.0.2",
    "node-uuid": "^1.4.7",
    "qs": "^6.3.0",
    "redirect-https": "^1.1.1"
  },
  "devDependencies": {
    "@microsoft/ts-command-line": "^2.0.2",
    "@types/bcryptjs": "^2.3.30",
    "@types/bluebird": "^3.0.36",
    "@types/body-parser": "^0.0.33",
    "@types/boom": "^0.0.32",
    "@types/express": "^4.0.33",
    "@types/joi": "^9.0.33",
    "@types/jwt-simple": "^0.5.32",
    "@types/node": "^6.0.52",
    "@types/node-uuid": "^0.0.28",
    "@types/qs": "^6.2.30",
    "@types/webpack": "^3.0.1",
    "alsatian": "^1.1.0",
    "barrelsby": "^0.0.3",
    "concurrently": "^3.1.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "dos2unix": "^1.1.2",
    "if-env": "^1.0.0",
    "mkdirp": "^0.5.1",
    "pkg": "^4.1.0",
    "rimraf": "^2.6.1",
    "start-server-webpack-plugin": "^2.2.0",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tap-bark": "^1.0.0",
    "ts-loader": "^2.2.1",
    "ts-node": "^3.1.0",
    "typescript": "^2.3.2",
    "uglify-es": "^3.0.20",
    "webpack": "^3.0.0"
  }
}
